{
  "manifest_version": 3,
  "name": "Moodle Pro (HTL Spengergasse)",
  "version": "1.3",
  "description": "Moodle Theme for HTL Spengergasse and Moodle Tools (Features)",
  "icons": {
    "128": "icons/SPG-Premium.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://moodle.spengergasse.at/*"
      ],
      "js": [
        "content.js"
      ]
    },
    {
      "matches": [
        "*://moodle.spengergasse.at/*"
      ],
      "css": [
        "content.css"
      ],
      "run_at": "document_start"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "admin@moodleprospg.com"
    }
  }
}